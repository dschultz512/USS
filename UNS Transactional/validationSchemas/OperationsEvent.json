        "@listVersionID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@name": {
          "$ref": "#/$defs/xsd:string"
        }
      },
      "type": [
        "object",
        "string",
        "number",
        "boolean"
      ]
    },
    "EventsType": {
      "additionalProperties": false,
      "properties": {
        "Event": {
          "items": {
            "$ref": "#/$defs/SingleEventType"
          },
          "type": "array"
        }
      },
      "type": "object"
    },
    "TransGetType": {
      "additionalProperties": false,
      "properties": {
        "Expression": {
          "items": {
            "$ref": "#/$defs/xsd:token"
          },
          "type": "array"
        }
      },
      "type": "object"
    },
    "TransApplicationAreaType": {
      "additionalProperties": true,
      "properties": {
        "CreationDateTime": {
          "$ref": "#/$defs/DateTimeType"
        }
      },
      "required": [
        "CreationDateTime"
      ],
      "type": "object"
    },
    "TransSenderType": {
      "additionalProperties": false,
      "properties": {
        "LogicalID": {
          "$ref": "#/$defs/IdentifierType"
        }
      },
      "type": "object"
    },
    "IdentifierType": {
      "additionalProperties": false,
      "properties": {
        "$": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@schemeAgencyID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@schemeAgencyName": {
          "$ref": "#/$defs/xsd:string"
        },
        "@schemeDataURI": {
          "$ref": "#/$defs/xsd:anyURI"
        },
        "@schemeID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@schemeName": {
          "$ref": "#/$defs/xsd:string"
        },
        "@schemeURI": {
          "$ref": "#/$defs/xsd:anyURI"
        },
        "@schemeVersionID": {
          "$ref": "#/$defs/xsd:normalizedString"
        }
      },
      "type": [
        "object",
        "string",
        "number",
        "boolean"
      ]
    },
    "DateTimeType": {
      "additionalProperties": false,
      "properties": {
        "$": {
          "$ref": "#/$defs/xsd:dateTime"
        },
        "@format": {
          "$ref": "#/$defs/xsd:string"
        }
      },
      "type": [
        "object",
        "string",
        "number",
        "boolean"
      ]
    },
    "xsd:anyURI": {
      "type": "string"
    },
    "xsd:base64Binary": {
      "type": "string"
    },
    "xsd:dateTime": {
      "type": "string"
    },
    "xsd:decimal": {
      "type": "number"
    },
    "xsd:language": {
      "pattern": "^([A-Za-z]{1,8}(\\-[0-9A-Za-z]{1,8})*)$",
      "type": "string"
    },
    "xsd:normalizedString": {
      "type": "string"
    },
    "xsd:string": {
      "type": "string"
    },
    "xsd:token": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "properties": {
    "GetBatchProductionRecord": {
      "$ref": "#/$defs/GetBatchProductionRecord"
    }
  },
  "type": "object"
