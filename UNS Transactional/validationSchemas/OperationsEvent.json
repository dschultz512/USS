    "EventTypeType": {
      "additionalProperties": false,
      "properties": {
        "$": {
          "enum": [
            "Alarm",
            "Control Recipe",
            "Equipment",
            "General",
            "Material",
            "Message",
            "Operator",
            "Procedural Execution",
            "Other"
          ],
          "type": "string"
        },
        "@OtherValue": {
          "$ref": "#/$defs/xsd:string"
        },
        "@languageID": {
          "$ref": "#/$defs/xsd:language"
        },
        "@listAgencyID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@listAgencyName": {
          "$ref": "#/$defs/xsd:string"
        },
        "@listID": {
          "$ref": "#/$defs/xsd:normalizedString"
