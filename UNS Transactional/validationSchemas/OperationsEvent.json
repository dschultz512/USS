{
  "title": "Rhize MDH - Operations Event",
  "description": "Schema based on B2MML-BatchML V0701",
  "$defs": {
    "GetBatchProductionRecord": {
      "$ref": "#/$defs/GetBatchProductionRecordType"
    },
    "GetBatchProductionRecordType": {
      "additionalProperties": true,
      "properties": {
        "ApplicationArea": {
          "$ref": "#/$defs/TransApplicationAreaType"
        },
        "DataArea": {
          "additionalProperties": true,
          "properties": {
            "BatchProductionRecord": {
              "items": {
                "$ref": "#/$defs/BatchProductionRecordType"
              },
              "minItems": 1,
              "type": "array"
            }
          },
          "required": [
            "BatchProductionRecord"
          ],
          "type": "object"
        }
      },
      "required": [
        "DataArea"
      ],
      "type": "object"
    },
    "BatchProductionRecordType": {
      "additionalProperties": true,
      "properties": {
        "Events": {
          "$ref": "#/$defs/EventsType"
        }
      },
      "required": [
        "Events"
      ],
      "type": "object"
    },
    "SingleEventType": {
      "additionalProperties": true,
      "properties": {
        "EventType": {
          "$ref": "#/$defs/EventTypeType"
        },
        "EventSubType": {
          "anyOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/$defs/EventSubTypeType"
            }
          ]
        }
      },
      "required": [
        "EventSubType",
        "EventType"
      ],
      "type": "object"
    },
    "EventSubType1Type": {
      "additionalProperties": false,
      "properties": {
        "$": {
          "enum": [
            "Allocation",
            "Application",
            "Consume",
            "Deallocation",
            "Equipment",
            "Message",
            "Mode Change",
            "Mode Command",
            "Modification",
            "Movement",
            "Parameter Data",
            "Process",
            "Process Data",
            "Produce",
            "Prompt",
            "Prompt Response",
            "Property Value Change",
            "Reconciliation",
            "Security",
            "State Change",
            "State Command",
            "Status Change",
            "System",
            "Target End Time",
            "Target Start Time",
            "Other"
          ],
          "type": "string"
        },
        "@languageID": {
          "$ref": "#/$defs/xsd:language"
        },
        "@listAgencyID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@listAgencyName": {
          "$ref": "#/$defs/xsd:string"
        },
        "@listID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@listName": {
          "$ref": "#/$defs/xsd:string"
        },
        "@listSchemeURI": {
          "$ref": "#/$defs/xsd:anyURI"
        },
        "@listURI": {
          "$ref": "#/$defs/xsd:anyURI"
        },
        "@listVersionID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@name": {
          "$ref": "#/$defs/xsd:string"
        }
      },
      "type": [
        "object",
        "string",
        "number",
        "boolean"
      ]
    },
    "EventSubTypeType": {
      "additionalProperties": false,
      "properties": {
        "$": {
          "enum": [
            "Allocation",
            "Application",
            "Consume",
            "Deallocation",
            "Equipment",
            "Message",
            "Mode Change",
            "Mode Command",
            "Modification",
            "Movement",
            "Parameter Data",
            "Process",
            "Process Data",
            "Produce",
            "Prompt",
            "Prompt Response",
            "Property Value Change",
            "Reconciliation",
            "Security",
            "State Change",
            "State Command",
            "Status Change",
            "System",
            "Target End Time",
            "Target Start Time",
            "Other"
          ],
          "type": "string"
        },
        "@OtherValue": {
          "$ref": "#/$defs/xsd:string"
        },
        "@languageID": {
          "$ref": "#/$defs/xsd:language"
        },
        "@listAgencyID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@listAgencyName": {
          "$ref": "#/$defs/xsd:string"
        },
        "@listID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@listName": {
          "$ref": "#/$defs/xsd:string"
        },
        "@listSchemeURI": {
          "$ref": "#/$defs/xsd:anyURI"
        },
        "@listURI": {
          "$ref": "#/$defs/xsd:anyURI"
        },
        "@listVersionID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@name": {
          "$ref": "#/$defs/xsd:string"
        }
      },
      "type": [
        "object",
        "string",
        "number",
        "boolean"
      ]
    },
    "EventType1Type": {
      "additionalProperties": false,
      "properties": {
        "$": {
          "enum": [
            "Alarm",
            "Control Recipe",
            "Equipment",
            "General",
            "Material",
            "Message",
            "Operator",
            "Procedural Execution",
            "Other"
          ],
          "type": "string"
        },
        "@languageID": {
          "$ref": "#/$defs/xsd:language"
        },
        "@listAgencyID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@listAgencyName": {
          "$ref": "#/$defs/xsd:string"
        },
        "@listID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@listName": {
          "$ref": "#/$defs/xsd:string"
        },
        "@listSchemeURI": {
          "$ref": "#/$defs/xsd:anyURI"
        },
        "@listURI": {
          "$ref": "#/$defs/xsd:anyURI"
        },
        "@listVersionID": {
          "$ref": "#/$defs/xsd:normalizedString"
        },
        "@name": {
          "$ref": "#/$defs/xsd:string"
        }
      },
      "type": [
        "object",
        "string",
        "number",
